{
  "name": "paramind-word-addin",
  "version": "1.0.0",
  "description": "AI-powered Microsoft Word add-in for document interaction and assistance",
  "main": "src/taskpane/taskpane.js",
  "scripts": {
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "dev-server": "webpack serve --mode=development",
    "start": "office-addin-debugging start manifest.xml",
    "stop": "office-addin-debugging stop manifest.xml",
    "validate": "office-addin-manifest validate manifest.xml",
    "sideload": "office-addin-dev-settings sideload manifest.xml"
  },
  "keywords": [
    "Office Add-in",
    "Word",
    "AI",
    "OpenAI",
    "Document Assistant"
  ],
  "author": "Vanya",
  "license": "MIT",
  "dependencies": {
    "@types/office-js": "^1.0.376",
    "office-js": "latest",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.3",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.3",
    "office-addin-debugging": "^5.0.15",
    "office-addin-dev-certs": "^1.13.3",
    "office-addin-manifest": "^1.13.3",
    "office-addin-prettier-config": "^1.2.0",
    "style-loader": "^3.3.3",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "homepage": "https://localhost:3000/"
} 